{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Polska",
	"settings": {
		"custom": {
			"border": {
				"small": "1px",
				"medium": "2px",
				"large": "3px"
			},
			"menu-icon": {
				"scale": "1.3"
			}
		},
		"color": {
			"palette": [
				{
					"slug": "contrast",
					"name": "Contrast",
					"color": "#2f201b"
				},
				{
					"slug": "base",
					"name": "Base",
					"color": "#f4f4f2"
				},
				{
					"slug": "accent",
					"name": "Accent",
					"color": "#ff86a8"
				},
				{
					"slug": "accent-2",
					"name": "Accent 2",
					"color": "#ee4c7d"
				},
				{
					"slug": "accent-3",
					"name": "Accent 3",
					"color": "#9d0e31"
				},
				{
					"slug": "contrast-2",
					"name": "Contrast 2",
					"color": "#3e302b"
				},
				{
					"slug": "contrast-3",
					"name": "Contrast 3",
					"color": "#3f3f3e"
				},
				{
					"slug": "base-2",
					"name": "Base 2",
					"color": "#edece9"
				},
				{
					"slug": "base-3",
					"name": "Base 3",
					"color": "#a3a29e"
				}
			],
			"duotone": [
				{
					"name": "White / Accent",
					"slug": "white-and-accent",
					"colors": [ "#ffffff", "#ff86a8" ]
				},
				{
					"name": "White / Accent 2",
					"slug": "white-and-accent-2",
					"colors": [ "#ffffff", "#ee4c7d" ]
				},
				{
					"name": "White / Accent 3",
					"slug": "white-and-accent-3",
					"colors": [ "#ffffff", "#9d0e31" ]
				},
				{
					"name": "Base / Base 2",
					"slug": "base-and-base-2",
					"colors": [ "#fff", "#edece9" ]
				}
			]
		}
	},
	"styles": {
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--accent-2)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cormorant)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--cormorant)"
		},
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontWeight": "700",
					"fontStyle": "normal",
					"letterSpacing": "-0.01em",
					"lineHeight": "1",
					"textTransform": "none"
				}
			},
			"core/button": {
				"typography": {
					"fontWeight": "700",
					"fontStyle": "normal"
				},
				"color": {
					"background": "var:preset|color|accent",
					"text": "var:preset|color|base"
				},
				"border": {
					"radius": "4px"
				}
			},
			"core/quote": {
				"color": {
					"text": "var:preset|color|base"
				}
			},
			"core/pullquote": {
				"color": {
					"text": "var:preset|color|base"
				}
			}
		}
	}
}